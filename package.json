{
  "name": "tricket",
  "version": "1.0.0",
  "description": "",
  "main": "app.ts",
  "scripts": {
    "start-dev": "dotenv -e ./dotenv/.env.development -- ts-node ./app.ts",
    "start-test": "dotenv -e ./dotenv/.env.test -- ts-node ./app.ts",
    "start-prod": "dotenv -e ./dotenv/.env.production -- ts-node ./app.ts",
    "test": "jest",
    "migrate-dev": "dotenv -e ./dotenv/.env.development -- npx prisma migrate dev",
    "migrate-test": "dotenv -e ./dotenv/.env.test -- npx prisma migrate dev",
    "migrate-prod": "dotenv -e ./dotenv/.env.production -- npx prisma migrate dev",
    "push-dev": "dotenv -e ./dotenv/.env.development -- npx prisma db push --force-reset",
    "push-test": "dotenv -e ./dotenv/.env.test -- npx prisma db push --force-reset",
    "push-prod": "dotenv -e ./dotenv/.env.production -- npx prisma db push --force-reset",
    "migrate-reset-dev": "dotenv -e ./dotenv/.env.development -- npx prisma migrate reset dev",
    "migrate-reset-test": "dotenv -e ./dotenv/.env.test -- npx prisma migrate reset dev",
    "migrate-reset-prod": "dotenv -e ./dotenv/.env.production -- npx prisma migrate reset dev",
    "generate-dev": "dotenv -e ./dotenv/.env.development -- npx prisma generate dev",
    "generate-test": "dotenv -e ./dotenv/.env.test -- npx prisma generate dev",
    "generate-prod": "dotenv -e ./dotenv/.env.production -- npx prisma generate dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.5",
    "@types/supertest": "^6.0.2",
    "express": "^4.18.2",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "prisma": "^5.8.1",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@prisma/client": "^5.8.1",
    "@types/bcrypt": "^5.0.2",
    "@types/dotenv": "^8.2.0",
    "bcrypt": "^5.1.1",
    "express-jwt": "^8.4.1",
    "express-jwt-permissions": "^1.3.7",
    "jsonwebtoken": "^9.0.2"
  },
  "prisma": {
    "seed": "./prisma/seed/seed.sh"
  }
}
